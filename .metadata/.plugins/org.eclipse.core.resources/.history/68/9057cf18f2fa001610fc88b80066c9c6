package com.reciperex.model;

import com.reciperex.storage.service.IdService;

public class Attributor {

	private Integer id;
	private String firstName;
	private String lastName;
	private Integer userId;
	
	public Attributor(String firstName, String lastName) {
		super();
		init(firstName, lastName);
	}
	
	public Attributor(String firstName, String lastName, Integer userId) {
		super();
		init(firstName, lastName);
		this.userId = userId;
	}
	
	private void init(String firstName, String lastName){
		this.id = IdService.generateId("attributor");
		this.firstName = firstName;
		this.lastName = lastName;
	}
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}

	public String getFirstName() {
		return firstName;
	}
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}
	
	public String getLastName() {
		return lastName;
	}
	public void setLastName(String lastName) {
		this.lastName = lastName;
	}
	
	public Integer getUserId() {
		return userId;
	}
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	
	
}
